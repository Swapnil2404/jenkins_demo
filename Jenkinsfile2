pipeline {
     agent any
     stages {
         stage('Build') {
             steps {
                 sh 'echo "Hello World"'
                 sh '''
                     echo "Multiline shell steps works too"
                     ls -lah
                 '''
             }
         }  
         stage('exicution') {
             steps {
                 sh '''
                    cd ./real-estate-management/frontend
                    sh 
                    npm install
                    npm run Build
                 '''
             }
         } 
          stage('Upload to AWS') {
              steps {
                  withAWS(region:'ap-south-1',credentials:'AKIAS5WLJKZP4YH5LE4A (task for practice)')
              }
          } 
         
         stage('upload data') {
             steps {
                 sh 'aws s3 cp --recursive ./real-estate-management/frontend/www s3://statichost24/ --region "ap-south-1"'
                 
             }
         }         
         
         }
     }

